#include"bits/stdc++.h"

using namespace std;

void printres(vector<int>v);

int main()
{
    vector<int>v1={5, 3, 7, 9, 1};
    vector<int>v2=v1;

    // Reversing from begining to end
    //             Method-1
    reverse(v1.begin(), v1.end());
    printres(v1);

    // Reversing from index-0 to index-4
    //             Method-2
    reverse(v2.begin(), v2.begin()+3);
    printres(v2);

    return 0;
}

void printres(vector<int>v)
{
    for(int i=0; i<v.size(); i++)
        cout << v[i] << " ";
    cout << endl;
}
